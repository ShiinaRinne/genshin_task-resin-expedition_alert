    "WW_ID": {
      "$id": "#/properties/WW_ID",
      "default": "",
      "description": "",
      "examples": [
        ""
      ],
      "title": "企业微信id",
      "type": "string"
    },
    "CQHTTP_IP": {
      "$id": "#/properties/CQHTTP_IP",
      "default": "",
      "description": "",
      "examples": [
        ""
      ],
      "title": "cqhttp的ip",
      "type": "string"
    },
    "CQHTTP_USER_ID": {
      "$id": "#/properties/CQHTTP_USER_ID",
      "default": "",
      "description": "",
      "examples": [
        ""
      ],
      "title": "推送的对象(qq号码)",
      "type": "string"
    },
    "CQHTTP_TOKEN": {
      "$id": "#/properties/CQHTTP_TOKEN",
      "default": "",
      "description": "",
      "examples": [
        ""
      ],
      "title": "cqhttp的access_token，未设置不填",
      "type": "string"
    }
  },
  "required": [
    "UID",
    "COOKIE",
    "RESIN_ALERT_NUM",
    "RECEIVE_RESIN_DATA",
    "RECEIVE_BOSS_COUNT",
    "RECEIVE_TASK_NUM",
    "REVEIVE_EXPEDITION_NUM",
    "EXPEDITION_COMPLETE_ALERT",
    "INCOMPLETE_ALERT",
    "SLEEP_TIME",
    "ALERT_SUCCESS_SLEEP_TIME"
  ],
  "title": "Genshin-Alert Config",
  "type": "object"
}
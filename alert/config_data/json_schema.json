    },
    "WW_APP_USERID": {
      "$id": "#/properties/WW_APP_USERID",
      "default": "",
      "description": "",
      "examples": [
        ""
      ],
      "title": "企业微信接收消息的用户id",
      "type": "string"
    },
    "WW_BOT_KEY": {
      "$id": "#/properties/WW_BOT_KEY",
      "default": "",
      "description": "",
      "examples": [
        ""
      ],
      "title": "企业微信机器人Key",
